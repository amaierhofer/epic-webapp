:javascript 
  $(function() {
    $.get('/mockups/data/makeit_phone2browser_sample01.txt', function(text) {
      var data = $(text);
      $.nmk.log(data);
      var res = $('.results');
      res.find('li').remove();
      data.find('data').children().each(function() {
        var entry = $(this);
        var action = entry.find('action').text();
        var clazz = entry.find('class').text();
        var pkg = entry.find('package').text();
        $.nmk.log(action);
        var link = '<a href="#" rel="external">' + action + '</a>';
        var li = '<li>' + link + '</li>';
        res.append(li);
      });
      res.listview('refresh');
    });
  });

%ul.results{'data-role' => 'listview', 'data-inset' => 'true', 'data-theme' => 'a'}
  %li a
  %li b
